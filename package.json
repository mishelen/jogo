{
  "name": "joggy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run test",
    "start": "npm run clean && npm run transpile",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "transpile": "NODE_ENV=PRODUCTION webpack -p --config ./webpack/webpack.config.base.babel.js",
    "hmr:server": "webpack-dev-server --config ./webpack/webpack.config.dev-server.hmr.babel.js",
    "clean": "rimraf -rf ./dist & rimraf -rf ./public",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "setupFiles": ["<rootDir>/src/tempPolyfills.js", "<rootDir>/src/setupTests.js"],
    "moduleFileExtensions": ["js"],
    "moduleDirectories": ["node_modules"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/mocks/fileMock.js",
      "\\.(scss|css)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"],
    "{.prettier,.babelrc,.stylelintrc,.eslintrc}": ["prettier --write", "git add"],
    "*.scss": ["prettier --write", "stylelint --fix -s scss", "git add"],
    "*.md": ["prettier --write", "git add"]
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.2",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.9.2",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-bare-webpack-plugin": "^0.1.3",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
